function doingquest()
    return game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.Enabled
end

function checklevel()
    local lv = game:GetService("Players").zxcv11112333.PlayerData.Experience.Level.Value
    if lv == 1 and lv <= 9 then
        questgive = game:GetService("Workspace")["Npc_Workspace"].QuestGivers:FindFirstChild("1"):FindFirstChild("1")
        levelq = "Level 1"
        namemon = "Thief"
        monfoleder = "Starter Island"
        cframemon = CFrame.new(-2426.29688, 13.5969877, -3237.31738, -0.593567252, -4.66680206e-08, -0.804784417, -9.20162346e-09, 1, -5.12015887e-08, 0.804784417, -2.29862618e-08, -0.593567252)
    elseif lv == 10 and lv <= 24 then
        questgive = game:GetService("Workspace")["Npc_Workspace"].QuestGivers:FindFirstChild("1"):FindFirstChild("1")
        levelq = "Level 1"
        namemon = "Thief"
        monfoleder = "Starter Island"
        cframemon = CFrame.new(-2426.29688, 13.5969877, -3237.31738, -0.593567252, -4.66680206e-08, -0.804784417, -9.20162346e-09, 1, -5.12015887e-08, 0.804784417, -2.29862618e-08, -0.593567252)
    elseif lv == 25 and lv <= 39 then
        questgive = game:GetService("Workspace")["Npc_Workspace"].QuestGivers:FindFirstChild("1"):FindFirstChild("1")
        levelq = "Level 1"
        namemon = "Thief"
        monfoleder = "Starter Island"
        cframemon = CFrame.new(-2426.29688, 13.5969877, -3237.31738, -0.593567252, -4.66680206e-08, -0.804784417, -9.20162346e-09, 1, -5.12015887e-08, 0.804784417, -2.29862618e-08, -0.593567252)
    end
end

function abandonquest()
    pcall(function()
        x= game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.ExitButton.Button
        x.Position=UDim2.new(0,-800,0,-700);x.Size=UDim2.new(5000, 5000, 5000, 5000)
        x.BackgroundTransparency=1;
        task.wait(.03)
        game:service'VirtualInputManager':SendMouseButtonEvent(795,595,0,true,game,1);
        game:service'VirtualInputManager':SendMouseButtonEvent(795,595,0,false,game,1);
        x.Size=UDim2.new(1, 0, 1, 0)
        x.Position=UDim2.new(0.5,0,0.5,0)
    end)
end

task.spawn(function()--autofarmlevel
    while task.wait() do
        pcall(function()
            if getgenv().Config.autofarmlevel then
                if (doingquest()==false) then
                    spawn(function()
                        checklevel()
                        getgenv().noclip = true
                        unequip()
                        twochest(game:GetService("Workspace")["Npc_Workspace"].QuestGivers:FindFirstChild("1"):FindFirstChild("1").HumanoidRootPart.CFrame)
                        print("ewhat")
                        if (game:GetService("Workspace")["Npc_Workspace"].QuestGivers:FindFirstChild("1"):FindFirstChild("1").HumanoidRootPart.Position - game.Players.localPlayer.Character.HumanoidRootPart.Position).Magnitude  <= 3 then
                            game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.QuestFunction:InvokeServer(game:GetService("Workspace")["Npc_Workspace"].QuestGivers:FindFirstChild("1"):FindFirstChild("1"),levelq)
                        end
                    end)
                elseif (doingquest()==true) then
                    checklevel()
                    if not string.find(game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Quest.Quest.Text,namemon) then
                        abandonquest()
                    end
                    if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - cframemon.Position).magnitude >= 600 then
                        checklevel()
                        twochest(cframemon)
                    end
                    spawn(function()
                        if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - cframemon.Position).magnitude >= 600 then
                            checklevel()
                            twochest(cframemon)
                        end
                    end)
                    getgenv().noclip = true
                    for i,v in pairs(game:GetService("Workspace")["NPC Zones"][monfoleder]:GetChildren()) do
                        if v:FindFirstChild("NPCName").Value == namemon and v:FindFirstChild("Humanoid") then
                            if v.Humanoid.Health > 0 then
                                repeat task.wait()
                                    if string.find(game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.MainFrame.Frame.Main.List.QuestFrame.Quest.Quest.Text,namemon) then
                                        if v:FindFirstChild("NPCName").Value == namemon then 
                                            if v.Humanoid.Health == 0 then
                                                unequip()
                                            else
                                                equip(getgenv().Config._selectwaepon)
                                            end
                                            checklevel()
                                            UseFast = true
                                            _groupMon = true
                                            getgenv().noclip = true
                                            twochest(v.HumanoidRootPart.CFrame * CFrame.new(0,0,10))
                                            PosMon = v.HumanoidRootPart.CFrame
                                            v.Humanoid.JumpPower = 0
                                            v.Humanoid.WalkSpeed = 0
                                            v.Humanoid.Sit = true
                                             v.HumanoidRootPart.CanCollide = false
                                            v.HumanoidRootPart.Size = Vector3.new(20,20,20)
                                            v.Humanoid:ChangeState(14)
                                            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius",  math.huge)
                                            game:GetService'VirtualUser':CaptureController()
                                            game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672)) 
                                        end
                                    else
                                        game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer('AbandonQuest')
                                    end
                                until not v.Parent or not v.Humanoid.Health <= 0 or not v.Humanoid == 0 or getgenv().Config.autofarmlevel == false or game:GetService("Players").LocalPlayer.PlayerGui.QuestGui.Enabled == false
                                _groupMon = false
                                UseFast = false
                                getgenv().noclip = false
                                unequip()
                                checklevel()
                            end
                        end
                    end
                    for i,v in pairs(game:GetService("Workspace")["NPC Zones"][monfoleder]:GetChildren()) do
                        if not v:FindFirstChild("NPCName").Value == namemon then
                            checklevel()
                            _groupMon = false
                            UseFast = false
                            unequip()
                            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - cframemon.Position).magnitude >= 600 then
                                getgenv().noclip = true
                                checklevel()
                                twochest(cframemon)
                            end
                        end
                    end
                end
            end
        end)
    end
end)

------------------------------------------------< Function Setting >

function equip(typ) --- equip tool
    for i, v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
        if(v:IsA('Tool'))then
            if(v.ToolTip==typ or v.Name==typ )then
                game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
            end
        end
    end
end

function unequip() --- equip tool
    game.Players.LocalPlayer.Character.Humanoid:UnequipTools()
end

if getgenv().noclip then
    local a=Instance.new("Part");
    a.Name="epicbingus";
    a.Parent=game.Workspace;
    a.Anchored=true;
    a.Transparency=1;
    a.Size=Vector3.new(10,0.5,10);
end
game:GetService("RunService").Heartbeat:Connect(function()
   pcall(function()
        if not game:GetService("Workspace"):FindFirstChild("epicbingus") and getgenv().noclip then
            local a = Instance.new("Part")
            a.Name = "epicbingus"
            a.Parent = game.Workspace
            a.Anchored = true 
            a.Transparency = 1
            a.Size = Vector3.new(40,0.5,40)
        end
    end)
end)

function changestate()
   game.Workspace["epicbingus"].CFrame = CFrame.new(game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.X,-4,game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame.Z)
end

spawn(function()
    game:GetService("RunService").Heartbeat:Connect(function()
        pcall(function()
            if getgenv().noclip then
                if game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.Position.Y < 0 then
                    changestate()
                end
            end
        end)
    end)
end)

function Click()--Click function
    local VirtualUser = game:GetService('VirtualUser')
    VirtualUser:CaptureController()
    VirtualUser:ClickButton1(Vector2.new(851, 158), game:GetService("Workspace").Camera.CFrame)
end

local VirtualUser = game:GetService('VirtualUser')
spawn(function()---------- anti afk
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        pcall(function()
            VirtualUser:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
            task.wait(1)
            VirtualUser:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        end)
    end)
end)

spawn(function()------body velocity
    game:GetService("RunService").Stepped:Connect(function()
        pcall(function()
        if  _statetp or getgenv().noclip then
            if not game.Players.localPlayer.Character.HumanoidRootPart:FindFirstChild('bv') then
                local bv = Instance.new("BodyVelocity",game.Players.LocalPlayer.Character.HumanoidRootPart)
                bv.Name = 'bv'
                bv.MaxForce = Vector3.new(100000,100000,100000)
                bv.Velocity = Vector3.new(0,0,0)
            end
        else
            if game.Players.localPlayer.Character.HumanoidRootPart:FindFirstChild('bv') then
                game.Players.localPlayer.Character.HumanoidRootPart:FindFirstChild('bv'):Destroy()
                game.Players.localPlayer.Character.HumanoidRootPart:FindFirstChild('bv'):Destroy()
                game.Players.localPlayer.Character.HumanoidRootPart:FindFirstChild('bv'):Destroy()
            end
         end
        end)
    end)
end)

game:GetService("RunService").Heartbeat:Connect(function()
    pcall(function()
        if game:GetService("Players").LocalPlayer.Character:FindFirstChild("Humanoid") then
            if getgenv().noclip then
                setfflag("HumanoidParallelRemoveNoPhysics", "False")
                setfflag("HumanoidParallelRemoveNoPhysicsNoSimulate2", "False")
                game:GetService("Players").LocalPlayer.Character.Humanoid:ChangeState(11)
            end
        end
    end)
end)


local mt = getrawmetatable(game)
local nm = mt.__namecall
setreadonly(mt,false)
mt.__namecall = newcclosure(function(self,...)
    if getnamecallmethod() == 'Kick' or getnamecallmethod() == 'kick' and not Hopping then
        game:GetService("TeleportService"):Teleport(game.PlaceId)
    end

    return nm(self,...)
end)

local lp,po,ts = game:GetService('Players').LocalPlayer,game.CoreGui.RobloxPromptGui.promptOverlay,game:GetService('TeleportService')
 
po.ChildAdded:connect(function(a)
    if a.Name == 'ErrorPrompt' and not Hopping then
        repeat
            game:GetService("TeleportService"):Teleport(game.PlaceId)
            wait(2)
        until false
    end
end)
