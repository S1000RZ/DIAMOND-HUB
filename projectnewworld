Island = {
    "Start Island",
    "Buggy Island",
    "Along Park",
    "Dessert Island",
    "Sea Restaurant",
    "Lounge Town",
    "Junggle",
    "Marine Island",
    "Sky Island",
    "Marine Ford",
}

FIsland={}
for i, v in pairs(game:GetService("Workspace")["Npc_Workspace"]["Spawn Setters"]:GetChildren()) do
    if v:IsA "Model" then
        if not table.find(FIsland, tostring(v)) then
            table.insert(FIsland, tostring(v))
        end
    end
end


tabs.teleport:Label("- [ Teleport ]")

tabs.teleport:DropDown(Island,function(x)
    toplace=x
end)
tabs.teleport:Toggle("toplace",false,function(x)
    _statetp = x
    if _statetp then
        repeat wait()
            getgenv().noclip = true
                if toplace == "Start Island" then
                    twochest(CFrame.new(-2249.72217, 10.5708256, -3295.19287, -0.999996662, 0, -0.00258965837, 0, 1, 0, 0.00258965837, 0, -0.999996662))
                end
                if toplace == "Buggy Island" then
                    twochest(CFrame.new(-2159.07178, 14.6281414, -631.17749, 0.74639833, 5.03110016e-08, 0.665499449, -9.26306711e-08, 1, 2.82920993e-08, -0.665499449, -8.27628384e-08, 0.74639833))
                end
                if toplace == "Along Park" then
                    twochest(CFrame.new(-4898.57617, 17.9078178, -2833.3291, 0.948254764, 8.60840643e-08, 0.317510515, -1.07269031e-07, 1, 4.92402705e-08, -0.317510515, -8.07513629e-08, 0.948254764))
                end
                if toplace == "Dessert Island" then
                    twochest(CFrame.new(-5262.4375, 31.5943222, -686.362366, 0.424933434, 7.01908007e-08, -0.905224621, -6.82305767e-08, 1, 4.55106353e-08, 0.905224621, 4.2425004e-08, 0.424933434))
                end
                if toplace == "Sea Restaurant" then
                    twochest(CFrame.new(-5974.56641, 54.457325, 2399.95557, -0.999611974, -2.80465535e-08, -0.02785502, -2.89488327e-08, 1, 3.19887334e-08, 0.02785502, 3.27826903e-08, -0.999611974))
                end
                if toplace == "Lounge Town" then
                    twochest(CFrame.new(-2857.88696, 18.3296089, 1927.89368, -0.761887252, 5.34769811e-08, -0.647709668, 7.49817488e-08, 1, -5.63625724e-09, 0.647709668, -5.28605959e-08, -0.761887252))
                end
                if toplace == "Junggle" then
                    twochest(CFrame.new(37.2348671, 10.3699532, 2294.50024, 0.149043113, 5.95481211e-08, 0.988830686, -8.94042032e-08, 1, -4.674515e-08, -0.988830686, -8.14385785e-08, 0.149043113))
                end
                if toplace == "Marine Island" then
                    twochest(CFrame.new(543.10144, 101.612045, 5678.46973, -0.732080519, 6.12752586e-08, -0.681218088, 1.90867695e-08, 1, 6.94376823e-08, 0.681218088, 3.78317218e-08, -0.732080519))
                end
                if toplace == "Sky Island" then
                    twochest(CFrame.new(-2387.80688, 785.889893, 5553.43994, -0.0544116758, -1.01852919e-07, 0.998518586, 2.76421037e-08, 1, 1.03510317e-07, -0.998518586, 3.32333236e-08, -0.0544116758))
                end
                if toplace == "Marine Ford" then
                    twochest(CFrame.new(-6162.02441, 19.4918919, 6880.32617, 0.984825432, 7.72278863e-09, 0.173547804, -1.29553843e-08, 1, 2.90179614e-08, -0.173547804, -3.08260049e-08, 0.984825432))
                end
        until not _statetp
        stopchest()
        getgenv().noclip = false
    end
end)

